<script>
    <script> var initFnTicker = function() {
 var wrap = $('#js-tickerWrap');
 var LPIds = [];
 $('[data-iid]', wrap).each(function () {
 var id = $(this).data('iid');
 LPIds.push("" + id);
 var fn = getUpdateTickerFn($(this), id);
 LPIds.push("" + id);
 if (LPFn[id]) {
 LPFn[id].push(fn);
 } else {
 LPFn[id] = [fn];
 }
 });
 if (!LPIds.length) {
 return;
 }
 readApi();
 wrap.fadeIn();
 setInterval(readApi, 600000);
 function readApi(){
 $.ajax({
 url: pURL + "/trading/v1/quoteCurrent",
 type: "POST",
 data: JSON.stringify({instrumentId: LPIds}),
 dataType: "json",
 contentType: "application/json; charset=utf-8",
 error: function(){},
 success: function(d){
 if(d){
 for(var i in d.quotes){
 (LPFn[d.quotes[i].instrumentId] ) && (LPFn[d.quotes[i].instrumentId].forEach(function(fn){fn(d.quotes[i])}));
 }
 }
 }
 });
 }
 function getUpdateTickerFn(jObj, id) {
 var priceBuy = $('.js-priceBuy', jObj);
 var changePrice = $('.js-changePrice', jObj);
 var digit = jObj.data('dg');
 var startPrice = jObj.data('start');
 return function (d) {
 var b = d.ask ? (+d.ask).toFixed(digit) : '-';
 priceBuy.text(b);
 if (startPrice && d.ask){
 var change = (((+d.ask - startPrice)/startPrice) * 100).toFixed(2);
 if(changePrice.text().replace('+', '') != (change + '%')){
 changePrice.text((change > 0 ? '+' : '') + change + '%');
 if(change > 0){
 changePrice.removeClass('grow-down');
 if(!changePrice.hasClass('grow-up')){
 setTimeout(function () { changePrice.addClass('grow-up'); }, 30);
 }
 }else{
 changePrice.removeClass('grow-up');
 if(!changePrice.hasClass('grow-down')){
 setTimeout(function () { changePrice.addClass('grow-down'); }, 30);
 }
 }
 }
 }
 };
 }
 }
 window.pageReadyFN.push(initFnTicker);</script> 
</script>